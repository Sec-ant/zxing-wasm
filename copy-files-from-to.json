{
  "copyFiles": [
    {
      "from": "./src/reader/*.wasm",
      "to": "./dist/reader/"
    },
    {
      "from": "./src/writer/*.wasm",
      "to": "./dist/writer/"
    },
    {
      "from": "./src/full/*.wasm",
      "to": "./dist/full/"
    },
    {
      "from": "./src/imageData.d.ts",
      "to": "./dist/es/types/"
    },
    {
      "from": "./src/imageData.d.ts",
      "to": "./dist/cjs/types/"
    },
    {
      "from": "./src/wasm.d.ts",
      "to": "./dist/es/types/"
    },
    {
      "from": "./src/wasm.d.ts",
      "to": "./dist/cjs/types/"
    }
  ],
  "copyFilesSettings": {
    "whenFileExists": "overwrite"
  }
}
